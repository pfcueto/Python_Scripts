def numprim_lista(a):
    lista_numprim = []
    for i in range(1,a+1):
        iii = 0
        for ii in range(1,i+1):
            x = i % ii
            if x == 0:
                iii = iii + 1
                
        if iii == 2 or i == 1:
                lista_numprim.append(ii)
    print(lista_numprim)
